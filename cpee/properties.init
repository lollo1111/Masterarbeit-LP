    <parallel_branch pass="" local="">
      <manipulate id="a13" label="Spiegel zur Bearbeitung vorbereiten"/>
      <choose mode="exclusive">
        <alternative condition="data.mirrorShape == "rund"">
          <manipulate id="a14" label="Spiegel rund zuschneiden"/>
          <_probability>
            <_probability_min/>
            <_probability_max/>
            <_probability_avg/>
          </_probability>
        </alternative>
        <otherwise>
          <manipulate id="a15" label="Spiegel eckig zuschneiden"/>
        </otherwise>
      </choose>
    </parallel_branch>
  </parallel>
  <manipulate id="a3" label="Spiegel auf Tür befestigen"/>
