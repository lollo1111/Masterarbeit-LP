</alternative>
<otherwise>
  <manipulate id="a6" label="Material zuschneiden mit Maschine B"/>
  <manipulate id="a8" label="Löcher vorbohren"/>
  <manipulate id="a21" label="Schubladen herstellen"/>
  <choose mode="exclusive">
    <alternative condition="data.tableStyel != "classic"">
      <manipulate id="a22" label="Kabeldurchführung einfügen"/>
      <_probability>
        <_probability_min/>
        <_probability_max/>
        <_probability_avg/>
      </_probability>
    </alternative>
    <otherwise>
      <manipulate id="a23" label="Tischplatte gravieren"/>
    </otherwise>
  </choose>
  <manipulate id="a24" label="Tischbeine herstellen"/>
</otherwise>
</choose>
<manipulate id="a16" label="Oberflächenbehandlung"/>
<manipulate id="a17" label="Qualitätskontrolle"/>
<loop mode="pre_test" condition="data.quality == "unacceptable"">
<_probability>
  <_probability_min/>
  <_probability_max/>
  <_probability_avg/>
</_probability>
<manipulate id="a18" label="Fehlende Bestandteile beilegen"/>
</loop>
<choose mode="exclusive">
<alternative condition="data.additionalEquipment">
  <manipulate id="a25" label="Zusatzausrüstung beilegen"/>
  <_probability>
    <_probability_min/>
    <_probability_max/>
    <_probability_avg/>
  </_probability>
</alternative>
<otherwise></otherwise>
</choose>
<manipulate id="a19" label="Produkt mittels Material ermitteln"/>
<choose mode="exclusive">
<alternative condition="data.product == "schreibtisch" and !data.additionalEquipment">
  <manipulate id="a30" label="In Box S verpacken"/>
  <_probability>
    <_probability_min/>
    <_probability_max/>
    <_probability_avg/>
  </_probability>
</alternative>
<alternative condition="(data.product == "schreibtisch" and data.additionalEquipment) or (data.product == "schrank" and !data.additionalEquipment)">
  <manipulate id="a29" label="In Box M verpacken"/>
  <_probability>
    <_probability_min/>
    <_probability_max/>
    <_probability_avg/>
  </_probability>
</alternative>
<otherwise>
  <manipulate id="a28" label="In Box L verpacken"/>
</otherwise>
</choose>
<manipulate id="a20" label="Pakethöhe messen"/>
<manipulate id="a26" label="Paket wiegen"/>
<choose mode="exclusive">
<alternative condition="(data.weight < data.maxWeight) and (data.height < data.maxHeight) and !data.express">
  <manipulate id="a32" label="Lieferschein für kleines Paket drucken"/>
  <_probability>
    <_probability_min/>
    <_probability_max/>
    <_probability_avg/>
  </_probability>
</alternative>
<alternative condition="(data.weight >= data.maxWeight) or(data.height >= data.maxHeight) and !data.express">
  <_probability>
    <_probability_min/>
    <_probability_max/>
    <_probability_avg/>
  </_probability>
  <manipulate id="a33" label="Lieferschein für großes Paket drucken"/>
  <manipulate id="a34" label="Paket auf Palette beladen und abwarten bis diese voll ist"/>
</alternative>
<otherwise>
  <manipulate id="a35" label="Expresslieferschein drucken"/>
</otherwise>
</choose>
<manipulate id="a27" label="Produkt versenden"/>
<stop id="a31"/>
</description>
